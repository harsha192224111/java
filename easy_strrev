import java.util.Scanner;

class prog{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        String s= sc.nextLine();
        String res="";
        int l=s.length();

        for(int i=l-1;i>=0;i--)
        res+=s.charAt(i);

        System.out.println(res);
    }
}
