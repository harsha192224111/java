import java.util.Scanner;

class prog{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter age  ");

        int a=sc.nextInt();
        
        if (a>=18)
        System.out.println("Eligible ");
        else
        System.out.println("Eligible afte "+(18-a)+" years");
    }
}
